# Local/private config goes in the include
[include]
        path = ~/.gitconfig.local
[hub]
        protocol = https
[branch]
        autosetupmerge = true
[color]
        diff = auto
        status = auto
        branch = auto
        ui = true
[core]
        excludesfile = /Users/emiliano.barbosa/.gitignore_global
        editor = vi
        filemode = true
        logallrefupdates = true
        whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
[help]
        browser = open
[apply]
        whitespace = nowarn
[mergetool]
        keepBackup = false
[diff]
        renames = copies
        mnemonicprefix = true
[difftool]
        prompt = false
[help]
        autocorrect = 1
[push]
        # See `git help config` (search for push.default)
        # for more information on different options of the below setting.
        #
        # Setting to git 2.0 default to suppress warning message
        default = simple
[user]
        name = AUTHORNAME
        email = AUTHOREMAIL
[credential]
        helper = GIT_CREDENTIAL_HELPER
